{
    "components": {
        "schemas": {
            "parameters":{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/RakipInitiative/Model-Metadata-Schema/blob/main/Parameters-Schema.json",
    "type": "object",
    "title": "Parameter-JSON-Exchange-Format Version 1.0.0",
    "description": "JSON exchange format of parameter data used and generated by FSK-ML models",
    "properties": {
        "generatorLanguage": {
            "type": "string",
            "description": "Script language the JSON file was created with",
            "enum": ["R", "Python"]
        },
        "parameters": {
            "type": "array",
            "description": "model parameters with metadata and data values",
            "items": {
                "type": "object",
                "properties": {
                    "metadata": { "$ref": "Path/To/ModelMetadataSchema/definitions/Parameter"},
                    "data":{
                        "type": "object",
                        "description": "serialized data of parameter value"
                    },
                    "modelId": {
                        "type": "string",
                        "description": "id of the model using this parameter"
                    }
                },
                "required": ["metadata", "data","modelId"]
            }
        }
    },
    "required": ["generatorLanguage"]
}
        }}}
