{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Language": {
      "type": "string"
    },
    "PackageList": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Package": {
            "type": "string"
          },
          "Version": {
            "type": "string"
          }
        },
        "required": [
          "Package",
          "Version"
        ]
      }
    }
  },
  "required": [
    "Language",
    "PackageList"
  ]
}
